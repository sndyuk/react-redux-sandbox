{
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": ["error", "never"]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "6",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "modules": true
    }
  },
  "extends": [
    "airbnb"
  ]
}
